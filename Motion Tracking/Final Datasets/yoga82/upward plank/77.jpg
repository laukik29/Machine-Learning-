<script type="text/javascript">
  var input = document.querySelector("#registerMobile");

  var iti = window.intlTelInput(input, {
    preferredCountries: [],
    utilsScript: "js/utils.js?1537727621611" // just for formatting/placeholders etc
  });





  document.getElementById('WURFL').value = navigator.userAgent || navigator.vendor || window.opera;

  // document.getElementById('deviceIdentifier').value = navigator.userAgent;


  document.getElementById('deviceIdentifier').value = navigator.userAgent;
  document.getElementById('regWURFL').value = navigator.userAgent || navigator.vendor || window.opera;
  document.getElementById('regDeviceIdentifier').value = navigator.userAgent;

  function verifyName(evt) {
    document.getElementById("registerName").style.borderBottom = "1px solid #AAB7B8";

    document.getElementById('registerNameCaution').innerHTML = "";
  }

  function verifyCity(evt) {
    document.getElementById("registerCity").style.borderBottom = "1px solid #AAB7B8";

    document.getElementById('registerCityCaution').innerHTML = "";
  }

  $("#registerCity").keydown(function(evt) {
    var e = event || evt; // for trans-browser compatibility
    var charCode = e.which || e.keyCode;
    console.log(charCode)
    if ((charCode > 31 && charCode < 186 && (charCode < 48 || charCode > 57)) || charCode == 8 || charCode == 188 || charCode == 190) {
      document.getElementById('registerCityCaution').style.display = "none";
      document.getElementById("registerCity").style.borderBottom = "1px solid #AAB7B8";
      return true;
    } else {
      document.getElementById("registerCity").style.borderBottom = "1px solid red";
      document.getElementById('registerCityCaution').innerHTML = " Only letters are allowed";
      document.getElementById('registerCityCaution').style.display = "block";
      return false;
    }
  })

  $("#registerName").keyup(function(evt) {
    if ($(this).val().match("^[a-zA-Z ]+$")) {
      document.getElementById('registerNameCaution').innerHTML = "";
      document.getElementById("registerName").style.borderBottom = "1px solid #AAB7B8";
    } else {
      $(this).val("");
      evt.preventDefault();
